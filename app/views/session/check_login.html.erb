<% unless session[:user_id].nil? %>
	<% uu = User.find(session[:user_id]) %>
	<h1>Logged in as <%= uu.username %></h1>
	<% if session[:isAdmin] %>
		You are an Admin. <br />
	<% end %>
	<%= link_to "logout", logout_path %> |
	<%= link_to "users", users_path %>
<% else %>
	<h1>Not logged in</h1>
	<script>
		window.location.replace("<%= login_path %>")
	</script>
<% end %>